{
  "build": {
    "beforeDevCommand": "{{fragment_before_dev_command}}",
    "beforeBuildCommand": "{{fragment_before_build_command}}",
    "devPath": "{{fragment_dev_path}}",
    "distDir": "{{fragment_dist_dir}}",
    "withGlobalTauri": "{{fragment_with_global_tauri}}"
  },
  "package": {
    "productName": "{{package_name}}",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "{{package_name}}",
        "width": 800
      }
    ]
  }
}
